[NSDNUSB_Install.NT.Services]
include     = input.inf
needs       = HID_SelSus_Inst.NT.Services

[Strings]
NSDN = "NyaSama Developer Network"
DiskName   = "NSDN USB Installation Disk"
NSDN.DeviceDesc = "NSDN USB"
NSDN.KeypadDesc = "NSDN Keypad"
NSDN.MouseDesc = "NSDN Mouse"
NSDN.CustomDesc = "NSDN Custom"
